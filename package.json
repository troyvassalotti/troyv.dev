{
	"name": "troyv.dev",
	"version": "2.5.0",
	"scripts": {
		"blog": "plop post",
		"build": "eleventy",
		"clean": "rm -rf node_modules package-lock.json",
		"debug": "DEBUG=* eleventy",
		"dev": "eleventy --serve --incremental",
		"lint::all": "npm run lint::dprint && npm run lint::eslint && npm run lint::njk",
		"lint::all-fix": "npm run lint::dprint-fix && npm run lint::eslint-fix && npm run lint::njk-fix",
		"lint::dprint": "dprint check",
		"lint::dprint-fix": "dprint fmt",
		"lint::eslint": "eslint",
		"lint::eslint-fix": "eslint --fix",
		"lint::njk": "djlint --lint \"src/\"",
		"lint::njk-fix": "djlint --reformat \"src/\"",
		"netlify::dev": "npx netlify dev",
		"netlify::deploy": "npm run lint::all && netlify deploy --build --context production --prod",
		"serve": "ELEVENTY_ENV=production eleventy --serve",
		"watch": "eleventy --quiet --watch"
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-fetch": "^3.0.0",
		"@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
		"@11ty/eleventy-plugin-rss": "^1.1.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"@chrisburnell/eleventy-cache-webmentions": "^1.1.2",
		"@netlify/functions": "^1.0.0",
		"@troyv/beats-per": "^1.0.0",
		"@troyv/cheatcodes": "^0.0.6",
		"@troyv/cloudysky": "^1.0.4",
		"@troyv/common-config": "^1.0.1",
		"@troyv/detune": "^1.0.1",
		"@troyv/typewriter": "^2.0.1",
		"dotenv": "^16.0.0",
		"dprint": "^0.34.5",
		"eleventy-plugin-youtube-embed": "^1.7.0",
		"es-module-shims": "^1.5.8",
		"eslint": "^8.35.0",
		"html-minifier-terser": "^7.1.0",
		"markdown-it-anchor": "^8.6.4",
		"markdown-it-footnote": "^3.0.3",
		"netlify-plugin-cache": "^1.0.3",
		"netlify-plugin-webmentions": "^1.1.1",
		"plop": "^3.1.2",
		"plvylist": "^3.0.2",
		"remark": "^14.0.2"
	}
}
