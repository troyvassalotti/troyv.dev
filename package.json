{
	"name": "troyv.dev",
	"version": "2.5.0",
	"scripts": {
		"blog": "plop post",
		"build": "eleventy",
		"clean": "rm -rf node_modules package-lock.json",
		"debug": "DEBUG=* eleventy",
		"dev": "eleventy --serve",
		"lint::all": "npm run lint::prettier && npm run lint::eslint",
		"lint::all-fix": "npm run lint::prettier-fix && npm run lint::eslint-fix",
		"lint::prettier": "prettier --check .",
		"lint::prettier-fix": "prettier --write .",
		"lint::eslint": "eslint",
		"lint::eslint-fix": "eslint --fix",
		"netlify::dev": "npx netlify dev",
		"netlify::deploy": "npm run lint::all && netlify deploy --build --context production --prod",
		"serve": "ELEVENTY_ENV=production eleventy --serve",
		"start": "npm run dev"
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-fetch": "^4.0.0",
		"@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"@chrisburnell/eleventy-cache-webmentions": "^1.2.5",
		"@troyv/beats-per": "^1.0.0",
		"@troyv/cheatcodes": "^0.0.6",
		"@troyv/cloudysky": "^1.0.5",
		"@troyv/common-config": "^1.1.0",
		"@troyv/detune": "^1.0.1",
		"@troyv/typewriter": "^2.0.1",
		"@troyv/word-salad": "^1.0.1",
		"common-tags": "^1.8.2",
		"dotenv": "^16.0.0",
		"eleventy-plugin-youtube-embed": "^1.9.1",
		"es-module-shims": "^1.8.2",
		"eslint": "^8.54.0",
		"html-minifier-terser": "^7.2.0",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-footnote": "^3.0.3",
		"netlify-plugin-cache": "^1.0.3",
		"netlify-plugin-webmentions": "^1.1.1",
		"plop": "^4.0.0",
		"plvylist": "^3.1.1",
		"prettier": "^3.1.0",
		"remark": "^15.0.1",
		"sass": "^1.69.5"
	},
	"prettier": "@troyv/common-config/.prettierrc.json"
}