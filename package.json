  "bugs": {
    "url": "https://github.com/troyvassalotti/personal-site/issues"
  },
  "homepage": "https://github.com/troyvassalotti/personal-site#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.8",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "@chrisburnell/eleventy-cache-webmentions": "^0.1.11",
    "eleventy-plugin-add-web-component-definitions": "^2.0.3",
    "prettier": "^2.5.1"
  },
