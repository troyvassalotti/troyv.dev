{
  "name": "troyv.dev",
  "version": "2.5.0",
  "description": "This is my portfolio, but it can really be whatever I want it to be.",
  "main": "_site/index.html",
  "scripts": {
    "build": "ELEVENTY_ENV=production eleventy",
    "watch": "eleventy --watch",
    "serve": "ELEVENTY_ENV=production eleventy --serve",
    "dev": "ELEVENTY_ENV=development eleventy --serve --quiet",
    "debug": "DEBUG=* eleventy",
    "pretty": "prettier --write .",
    "forestry:preview": "eleventy --serve",
    "deploy": "npm run pretty && netlify build && netlify deploy --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troyvassalotti/personal-site.git"
  },
