{
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "includes": ["**/*.{ts,js,json,md,toml,html,css}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "_site",
    ".cache",
    ".netlify"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.69.2.wasm",
    "https://plugins.dprint.dev/json-0.15.3.wasm",
    "https://plugins.dprint.dev/markdown-0.13.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.9.0.json@76c20538ea4231b3daa9b62059f90ec144200cab5477b7e95b730cbb3ed48957"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "printWidth": 100,
    "quoteProps": "consistent",
    "bracketSameLine": true,
    "tabWidth": 2
  }
}
